{
  "name": "dahlia-cli",
  "version": "1.2.8",
  "description": "CLI tool for Dahlia",
  "author": "forsigner",
  "bin": {
    "dahlia": "./bin/run",
    "dh": "./bin/run"
  },
  "bugs": "https://github.com/forsigner/dahlia/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.11",
    "@oclif/plugin-help": "^2.1.6",
    "@types/jest": "^24.0.11",
    "@types/webpack": "^4.4.26",
    "@types/webpack-dev-server": "^3.1.4",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.2",
    "cross-spawn": "^6.0.5",
    "dahlia-webpack-less": "^1.0.10",
    "dahlia-webpack-override": "^1.1.3",
    "dahlia-webpack-styled-components": "^1.0.3",
    "dahlia-webpack-styled-jsx": "^1.0.2",
    "download-git-repo": "^1.1.0",
    "fs-extra": "^7.0.1",
    "fs-jetpack": "^2.2.2",
    "json-ts": "^1.6.4",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-scripts": "^2.1.8",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/chokidar": "^2.1.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.4",
    "@types/prettier": "^1.16.1",
    "chai": "^4.2.0",
    "globby": "^9.1.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.4000"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/forsigner/dahlia",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dahlia",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "forsigner/dahlia",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
