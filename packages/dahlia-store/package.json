{
  "name": "dahlia-store",
  "version": "1.3.2",
  "description": "State management for Dahlia",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean:dist": "rimraf dist/*",
    "min": "terser dist/index.js -o dist/index.min.js",
    "build": "npm run clean:dist && rollup -c && npm run min",
    "build:watch": "npm run clean:dist && rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/.history/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/forsigner/dahlia.git"
  },
  "author": "forsigner",
  "license": "MIT",
  "bugs": {
    "url": "https://.com/forsigner/dahlia/issues"
  },
  "homepage": "https://github.com/forsigner/dahlia/blob/master/packages/dahlia-store/README.md",
  "devDependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.11",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.8.1",
    "jest": "^24.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "16.8.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "terser": "^3.17.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "dahlia-observable": "^1.1.1",
    "fast-deep-equal": "^2.0.1",
    "lodash.debounce": "^4.0.8"
  }
}
