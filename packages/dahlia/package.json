{
  "name": "dahlia",
  "version": "1.11.22",
  "description": "An opinionated React Framework for building modern web applications.",
  "main": "index.js",
  "module": "index.es.js",
  "files": [
    "config.d.ts",
    "form.d.ts",
    "form.js",
    "form.js.map",
    "http.d.ts",
    "http.js",
    "http.js.map",
    "index.d.ts",
    "index.es.js",
    "index.es.js.map",
    "index.js",
    "index.js.map",
    "index.min.js",
    "router.d.ts",
    "router.js",
    "router.js.map",
    "modal.d.ts",
    "modal.js",
    "modal.js.map",
    "store.d.ts",
    "store.js",
    "store.js.map"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "npm run clean:dist && tsc",
    "min": "terser index.js -o index.min.js",
    "build": "rollup -c && npm run min",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/.history/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/forsigner/dahlia.git"
  },
  "author": "forsigner",
  "license": "MIT",
  "bugs": {
    "url": "https://.com/forsigner/dahlia/issues"
  },
  "homepage": "https://github.com/forsigner/dahlia#README",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.8.1",
    "jest": "^24.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "16.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "terser": "^3.17.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/styled-components": "^4.1.12",
    "@types/styled-jsx": "^2.2.8",
    "antd": "^3.15.2",
    "checkok": "^1.1.1",
    "checkok-email": "^1.0.1",
    "checkok-max": "^1.0.1",
    "checkok-min": "^1.0.1",
    "checkok-number": "^1.0.0",
    "checkok-required": "^1.0.0",
    "checkok-string": "^1.0.1",
    "checkok-url": "^1.0.1",
    "dahlia-cli": "^1.2.10",
    "dahlia-form": "^1.0.4",
    "dahlia-http": "^1.0.12",
    "dahlia-modal": "^1.0.0",
    "dahlia-router": "^1.0.15",
    "dahlia-store": "^1.2.14",
    "gery": "^1.1.3",
    "gql-tag": "^1.0.1",
    "styled-components": "^4.1.3",
    "styled-jsx": "^3.2.0"
  }
